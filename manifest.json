{
  "manifest_version": 3,
  "name": "image-download-converter",
  "version": "1.0",
  "description": "Download any image in JPEG/JPG/PNG format.",
  "permissions": [
    "contextMenus",
    "downloads",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}